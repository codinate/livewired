{
    "name": "kodekeep/livewired",
    "description": "Reusable components for Laravel Livewire applications.",
    "keywords": [
        "kodekeep",
        "livewired"
    ],
    "homepage": "https://github.com/kodekeep/livewired",
    "license": "MPL-2.0",
    "authors": [
        {
            "name": "Brian Faust",
            "email": "brian@kodekeep.com",
            "homepage": "https://kodekeep.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4",
        "illuminate/contracts": "^6.0|^7.0",
        "illuminate/support": "^6.0|^7.0",
        "kodekeep/laravel-addresses": "^1.0",
        "kodekeep/laravel-notification-methods": "^1.0",
        "kodekeep/laravel-teams": "^1.0",
        "kodekeep/laravel-validation-rules": "^1.0",
        "laravel/airlock": "^1.0",
        "livewire/livewire": "^1.0",
        "pragmarx/google2fa-laravel": "^1.3",
        "spatie/laravel-personal-data-export": "^1.2"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "graham-campbell/analyzer": "^2.4",
        "orchestra/testbench": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "KodeKeep\\Livewired\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "KodeKeep\\Livewired\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test:coverage": "vendor/bin/phpunit --coverage-html=.coverage --coverage-clover=clover.xml",
        "format": "vendor/bin/php-cs-fixer fix ./"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "KodeKeep\\Livewired\\Providers\\LivewiredServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
